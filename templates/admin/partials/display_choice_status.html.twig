{% if real_value <= -4 %}
    <span class="label label-default">
{% elseif real_value <= -2 %}
    <span class="label label-danger">
{% elseif real_value <= 0 %}
    <span class="label label-warning">
{% else %}
    <span class="label label-success">
{% endif %}
{% set result = object.isExternal ? "Import - " : "" %}
{% set result = result ~ value %}
{{ result }}
</span>